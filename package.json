{
  "name": "typurejs",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.6.1",
  "type": "module",
  "description": "typure.js is a lightweight typescript library for building user interfaces. Based on native web components. Efficient, simple, flexible and no virtual DOM, no compile-time.",
  "engines": {
    "node": ">= 14.0.0",
    "pnpm": ">= 6.0.0"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "dev": "pnpm --dir ./example install && pnpm --dir ./example dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wangyewei/Typurejs.git"
  },
  "keywords": [
    "typescript",
    "html",
    "framework",
    "native",
    "html",
    "javsascript",
    "front-end"
  ],
  "author": "wangyewei <wangyewei1@foxmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wangyewei/Typurejs/issues"
  },
  "homepage": "https://github.com/wangyewei/Typurejs#readme",
  "dependencies": {
    "rollup": "^3.26.0",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "chalk": "^5.3.0",
    "execa": "^7.1.1",
    "minimist": "^1.2.8",
    "rollup-plugin-esbuild": "^5.0.0"
  }
}